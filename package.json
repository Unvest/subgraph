{
  "name": "@unvest/subgraph",
  "author": {
    "name": "JA <@ubinatus>",
    "url": "https://unvest.io"
  },
  "packageManager": "pnpm@9.15.0",
  "license": "GPL-3.0-or-later",
  "version": "1.0.0",
  "description": "Unvest Subgraph",
  "repository": "https://github.com/Unvest/subgraph",
  "scripts": {
    "clean": "rm -rf .turbo .next node_modules",
    "codegen": "graph codegen",
    "build:mainnet": "graph build --network mainnet",
    "build:optimism": "graph build --network optimism",
    "build:bsc": "graph build --network bsc",
    "build:polygon": "graph build --network matic",
    "build:arbitrum": "graph build --network arbitrum-one",
    "build:avalanche": "graph build --network avalanche",
    "build:base": "graph build --network base",
    "build:blast": "graph build --network blast-mainnet",
    "build:sepolia": "graph build --network sepolia",
    "build:optimism-sepolia": "graph build --network optimism-sepolia",
    "build:base-sepolia": "graph build --network base-sepolia",
    "deploy:mainnet": "pnpm build:mainnet && graph deploy --studio unvest-v3-mainnet",
    "deploy:optimism": "pnpm build:optimism && graph deploy --studio unvest-v3-optimism",
    "deploy:bsc": "pnpm build:bsc && graph deploy --studio unvest-v3-bsc",
    "deploy:polygon": "pnpm build:polygon && graph deploy --studio unvest-v3-polygon",
    "deploy:arbitrum": "pnpm build:arbitrum && graph deploy --studio unvest-v3-arbitrum",
    "deploy:avalanche": "pnpm build:avalanche && graph deploy --studio unvest-v3-avalanche",
    "deploy:base": "pnpm build:base && graph deploy --studio unvest-v3-base",
    "deploy:blast": "pnpm build:blast && graph deploy --studio unvest-v3-blast",
    "deploy:sepolia": "pnpm build:sepolia && graph deploy --studio unvest-v3-sepolia",
    "deploy:optimism-sepolia": "pnpm build:optimism-sepolia && graph deploy --studio unvest-v3-optimism-sepolia",
    "deploy:base-sepolia": "pnpm build:base-sepolia && graph deploy --studio unvest-v3-base-sepolia",
    "deploy:all-mainnets": "pnpm deploy:polygon && pnpm deploy:mainnet && pnpm deploy:optimism && pnpm deploy:bsc && pnpm deploy:arbitrum && pnpm deploy:avalanche && pnpm deploy:base && pnpm deploy:blast",
    "deploy:all-testnets": "pnpm deploy:sepolia && pnpm deploy:optimism-sepolia && pnpm deploy:base-sepolia",
    "deploy:all": "pnpm deploy:all-testnets && pnpm deploy:all-mainnets"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.93.0",
    "@graphprotocol/graph-ts": "^0.37.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "matchstick-as": "^0.6.0",
    "typescript": "^5.7.2"
  }
}
